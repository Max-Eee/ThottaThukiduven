{
    "manifest_version": 3,
    "author": "Sreecharan S. (sr2echa)",
    "name": "ThottaThukiduven",
    "version": "1.15",
    "description": "Your ultimate tool for Getting AI response, unblock Copy/Paste and keep the window always active. Yk where you can use...",
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "*://*/*"
    ],
    "permissions": ["contextMenus", "clipboardWrite", "clipboardRead", "storage", "tabs", "activeTab", "scripting", "downloads", "windows"],
    "action": {
      "default_icon": {
        "128": "icons/off.png"
      }
    },
    "icons": {
      "128": "icons/logo.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["data/inject/rightclickmenu.js","data/inject/isolated.js", "data/inject/content.js", "data/inject/main.js"]
      }
    ],
    "commands": {
      "show-overlay": {
          "suggested_key": {
              "default": "Alt+Shift+K"
          },
          "description": "Show OpenAI search overlay"
      },
       "search-with-openai": {
          "suggested_key": {
              "default": "Alt+Shift+Z"
          },
          "description": "Search with OpenAI"
      },
      "search-mcq": {
          "suggested_key": {
              "default": "Alt+Shift+X"
        },
        "description": "Search MCQ"
       }
    },
  
    "key": ""
}
